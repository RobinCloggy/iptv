<!--?xml version="1.0"?-->
<settings>
   
  <!-- for detailed info about the settings see http://webgrabplus.com/node/30 -->
   
  <filename>guide.xml</filename>
  <mode></mode>
  <postprocess grab="y" run="n">mdb</postprocess>
  <logging>on</logging>
  <retry time-out="5">4</retry>
  <timespan>0</timespan>
  <update>f</update>
 
  <!-- channels we want to grab from tvguide.co.uk (channels copied from tvguide.co.uk.channels.xml -->
  <channel site="tvguide.co.uk" site_id="66" update="i" xmltv_id="BBC News">BBC News</channel>
  <channel site="tvguide.co.uk" site_id="89" update="i" xmltv_id="BBC2">BBC2</channel>
  <channel site="tvguide.co.uk" site_id="154" update="i" xmltv_id="FOX">FOX</channel>
 
  <!-- channels we want to grab from mydigiguide.com (channels copied from mydigiguide.com.channels.xml -->
  <channel site="mydigiguide.com" site_id="6156" update="i" xmltv_id="Sky2">Sky2</channel>
 
</settings>
